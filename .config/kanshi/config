profile "no dock" {
  output eDP-1 mode 1920x1080@60Hz position 0,0 scale 1.2
}

profile "dock" {
  output eDP-1 disable
  output DP-7 mode 1920x1080@119.98Hz position 1920,1080 scale 1
  output DP-6 mode 1920x1080@60Hz position 1920,0 scale 1
  output DP-5 mode 1920x1080@60Hz position 0,1080 scale 1
  output DP-2 mode 1920x1080@60Hz position 3840,1080 scale 1
}
